{
  "hosting": {
    "site": "vekovshinin-ayu",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/src/**",
      "**/.idea/**",
      "**/config/**",
      "src/**",
      ".idea/**",
      "config/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
